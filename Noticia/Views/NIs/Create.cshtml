@model Noticia.Models.NI

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>NI</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Noticiasid" class="control-label"></label>
                <select asp-for="Noticiasid" class ="form-control" asp-items="ViewBag.Noticiasid"></select>
            </div>
            <div class="form-group">
                <label asp-for="Imagensid" class="control-label"></label>
                <select asp-for="Imagensid" class ="form-control" asp-items="ViewBag.Imagensid"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
